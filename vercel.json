{
    "version": 2,
    "builds": [
      {
        "src": "src/html/index.html",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "src/html"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/html/index.html"
      }
    ]
  }